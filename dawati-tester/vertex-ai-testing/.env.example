# Vertex AI Testing Configuration

# Google Cloud Project ID
GCP_PROJECT_ID=your-project-id

# Base URL for testing
BASE_URL=https://dawati-v01.vercel.app

# Browser mode (false = visible, true = headless)
HEADLESS=false

# Update baselines for visual regression (false = compare, true = update)
UPDATE_BASELINES=false

# Google Cloud Application Default Credentials
# Set this via: gcloud auth application-default login
# Or provide a service account key file path:
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Fine-Tuning Pipeline
FINE_TUNING_ENABLED=false
GCS_TRAINING_BUCKET=your-bucket-name
GCS_TRAINING_PREFIX=dawati-training-images
TUNING_REGION=europe-west4
TUNING_EPOCHS=4
TUNING_LR_MULTIPLIER=1.0
TUNING_ADAPTER_SIZE=4
TUNED_MODEL_ENDPOINT=
AB_TESTING_ENABLED=false
MIN_TRAINING_EXAMPLES=100

# Autopilot (fully automatic fine-tuning loop)
AUTOPILOT_ENABLED=false
AUTOPILOT_APPROVE_THRESHOLD=0.85
AUTOPILOT_REJECT_THRESHOLD=0.4
AUTOPILOT_TUNE_AT_COUNT=100
AUTOPILOT_AUTO_SWITCH=true

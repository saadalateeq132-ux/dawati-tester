{
  "name": "vertex-ai-testing",
  "version": "1.1.0",
  "description": "Production-grade Vertex AI + Playwright testing system for Dawati",
  "main": "src/index.ts",
  "scripts": {
    "test": "ts-node tests/auth-flow.test.ts",
    "test:headless": "HEADLESS=true ts-node tests/auth-flow.test.ts",
    "test:update-baselines": "UPDATE_BASELINES=true ts-node tests/auth-flow.test.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "feedback:review": "ts-node scripts/review-feedback.ts",
    "feedback:stats": "ts-node scripts/review-feedback.ts --stats",
    "feedback:auto": "ts-node scripts/review-feedback.ts --auto",
    "dataset:build": "ts-node scripts/build-dataset.ts",
    "dataset:build:dry": "ts-node scripts/build-dataset.ts --dry-run",
    "tuning:submit": "ts-node scripts/submit-tuning-job.ts",
    "tuning:submit:wait": "ts-node scripts/submit-tuning-job.ts --wait",
    "tuning:status": "ts-node scripts/check-tuning-status.ts",
    "tuning:list": "ts-node scripts/check-tuning-status.ts --list",
    "test:auth": "ts-node tests/auth-flow.test.ts",
    "test:marketplace": "ts-node tests/marketplace.test.ts",
    "test:events": "ts-node tests/event-creation.test.ts",
    "test:vendor": "ts-node tests/vendor-dashboard.test.ts",
    "test:bookings": "ts-node tests/bookings.test.ts",
    "test:account": "ts-node tests/account-settings.test.ts",
    "test:components": "ts-node tests/component-deep.test.ts",
    "test:misc": "ts-node tests/misc-pages.test.ts",
    "test:all": "ts-node tests/auth-flow.test.ts && ts-node tests/marketplace.test.ts && ts-node tests/event-creation.test.ts && ts-node tests/vendor-dashboard.test.ts && ts-node tests/bookings.test.ts && ts-node tests/account-settings.test.ts && ts-node tests/component-deep.test.ts && ts-node tests/misc-pages.test.ts",
    "typecheck": "tsc --noEmit",
    "test:ci": "HEADLESS=true npm run test:all"
  },
  "keywords": [
    "testing",
    "vertex-ai",
    "playwright",
    "gemini",
    "rtl",
    "i18n",
    "qa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.19.0",
    "@google-cloud/vertexai": "^1.7.0",
    "dotenv": "^16.4.7",
    "google-auth-library": "^9.15.1",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.50.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
